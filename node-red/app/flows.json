    {
        "id": "871449f1.a40ce8",
        "type": "function",
        "z": "3295b9b3.536026",
        "name": "setup commands",
        "func": "if(msg.topic == \"w\" || msg.topic == \"c\") {\n    msg.payload = \"/bankSet?params=\"+ msg.topic + \":\" + msg.payload;\n    node.send([[msg],[null]]);\n} else if(msg.topic == \"sys\") {\n    msg.payload = \"/\" + msg.payload;\n    node.send([[null],[msg]]);\n} else if(msg.topic == \"scene\") {\n    if(msg.payload == \"3500K\") {\n        msg.payload = \"/bankSet?params=w:255\";\n        node.send([[null],[msg]]);\n        msg.payload = \"/bankSet?params=c:0\";\n        node.send([[null],[msg]]);\n    } else if(msg.payload == \"6500K\") {\n        msg.payload = \"/bankSet?params=c:255\";\n        node.send([[null],[msg]]);\n        msg.payload = \"/bankSet?params=w:0\";\n        node.send([[null],[msg]]);\n    }\n}\n\nflow.set('command',true);\n\nvar timeout = setTimeout(function() {\n    flow.set('command',false);\n},3000)",
        "outputs": 2,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "x": 1760,
        "y": 440,
        "wires": [
            [
                "f96273b7.ba9cd",
                "a60a5299.11ae1"
            ],
            [
                "ca26cdce.b7833"
            ]
        ],
        "outputLabels": [
            "rate-limited with drops",
            "queued messages"
        ]
    },
